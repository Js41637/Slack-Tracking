      { label: 'Select &Next Team', command: 'window:select-next-team', accelerator: "Control+Tab"}
      { label: 'Select &Previous Team', command: 'window:select-previous-team', accelerator: "Control+Shift+Tab"}
      { type: 'separator' }
      { label: '&Sign in to Another Team…', command: 'window:signin' }
