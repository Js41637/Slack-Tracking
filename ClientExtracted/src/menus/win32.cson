'menu': [
  {
    label: '&File'
    submenu: [
      { label: '&Preferences…', command: 'application:show-settings', accelerator: "Control+," }
      { type: 'separator' }
      { label: '&Quit', command: 'application:quit', accelerator: "Control+Q" }
    ]
  }

  {
    label: '&Edit'
    submenu: [
      { label: '&Undo', command: 'core:undo', accelerator: "Control+Z" }
      { label: '&Redo', command: 'core:redo', accelerator: "Control+Y" }
      { type: 'separator' }
      { label: '&Cut', command: 'core:cut', accelerator: "Control+X" }
      { label: 'C&opy', command: 'core:copy', accelerator: "Control+C" }
      { label: '&Paste', command: 'core:paste', accelerator: "Control+V" }
      { label: 'Select &All', command: 'core:select-all', accelerator: "Control+A" }
      { label: 'Delete', command: 'core:delete', accelerator: 'Delete' }
    ]
  }

  {
    label: '&View'
    submenu: [
      { label: '&Reload', command: 'window:reload', accelerator: "Control+R" }
      { label: 'Toggle &Full Screen', command: 'window:toggle-full-screen', accelerator: "Control+Shift+F" }
      { type: 'separator' }
      { label: '&Actual Size', command: 'window:actual-size', accelerator: "Control+0" }
      { label: 'Zoom &In', command: 'window:zoom-in', accelerator: "Control+Plus" }
      { label: 'Zoom &Out', command: 'window:zoom-out', accelerator: "Control+-" }
      { type: 'separator' }
      {
        label: '&Developer'
        submenu: [
          { label: 'Run Tests…', command: 'application:run-specs', accelerator: "Control+Shift+T" }
          { label: 'Toggle Developer &Tools', command: 'window:toggle-dev-tools', accelerator: "Control+Shift+I" }
        ]
      }
    ]
  }

  {
    label: 'H&istory'
    submenu: [
      { label: '&Back', command: 'history:back', accelerator: 'Alt+Left' }
      { label: '&Forward', command: 'history:forward', accelerator: 'Alt+Right' }
    ]
  }

  {
    label: '&Window'
    submenu: [
      { label: 'Always Show &Menu Bar', command: 'window:auto-hide-menu-bar', type: 'checkbox', checked: true }
