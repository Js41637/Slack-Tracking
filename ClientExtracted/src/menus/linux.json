            {
              "label": "Toggle &Electron DevTools",
              "command": "window:toggle-all-dev-tools",
              "accelerator": "Control+Shift+Alt+I",
              "id": "menuitem-toggle-electron-devtools"
            },
            {
              "label": "&Reload Everything",
              "command": "window:reload-everything",
              "accelerator": "Control+Shift+R",
              "id": "menuitem-reload-all"
            }
          ]
        }
      ]
    },
    {
      "label": "H&istory",
      "submenu": [
        {
          "label": "&Back",
          "command": "history:back",
          "accelerator": "Alt+Left",
          "id": "menuitem-navigate-back"
        },
        {
          "label": "&Forward",
          "command": "history:forward",
          "accelerator": "Alt+Right",
          "id": "menuitem-navigate-forward"
        }
      ]
    },
    {
      "label": "&Window",
      "id": "menuitem-window",
      "submenu": [
        {
          "label": "Always Show &Menu Bar",
          "command": "window:auto-hide-menu-bar",
          "id": "menuitem-auto-hide-menu-bar",
          "type": "checkbox",
          "checked": true
        },
        {
          "type": "separator",
          "id": "team-list-start-separator"
        },
        {
          "label": "Select &Next Team",
          "command": "window:select-next-team",
          "accelerator": "Control+Tab",
          "id": "menuitem-select-next-team"
        },
        {
          "label": "Select &Previous Team",
          "command": "window:select-previous-team",
          "accelerator": "Control+Shift+Tab",
          "id": "menuitem-select-previous-team"
        },
        {
          "type": "separator"
        },
        {
          "label": "&Sign in to Another Team…",
          "command": "window:signin",
          "id": "menuitem-sign-in"
        }
      ]
    },
    {
      "label": "&Help",
      "id": "menuitem-help",
      "submenu": [
        {
          "label": "Keyboard Shortcuts",
          "command": "application:keyboard-shortcuts",
          "accelerator": "Control+/",
          "id": "menuitem-keyboard-shortcut"
        },
        {
          "label": "Open Help Center",
          "command": "application:help-center",
          "id": "menuitem-open-help-center"
        },
        {
          "type": "separator"
        },
        {
          "label": "Report Issue…",
          "command": "application:report-issue",
          "id": "menuitem-report-issue"
        },
        {
          "label": "Show &Logs in File Manager",
          "command": "application:prepare-and-reveal-logs",
          "id": "menuitem-reveal-logs"
        },
        {
          "label": "Reset App Data…",
          "command": "application:reset-app-data",
          "id": "menuitem-reset-app-data"
        },
        {
          "type": "separator"
        },
        {
          "label": "What's New…",
          "command": "application:release-notes",
          "id": "menuitem-release-notes"
        },
        {
          "label": "About Slack…",
          "command": "application:about",
          "id": "menuitem-about-slack"
        }
      ]
    }
