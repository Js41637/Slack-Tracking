        {
          "type": "separator"
        },
        {
          "label": "&Actual Size",
          "command": "window:actual-size",
          "accelerator": "Control+0",
          "id": "menuitem-zoom-reset"
        },
        {
          "label": "Zoom &In",
          "command": "window:zoom-in",
          "accelerator": "Control+Plus",
          "id": "menuitem-zoom-in"
        },
        {
          "label": "Zoom &Out",
          "command": "window:zoom-out",
          "accelerator": "Control+-",
          "id": "menuitem-zoom-out"
        },
        {
          "type": "separator"
        },
        {
          "label": "&Developer",
          "id": "menuitem-developer",
          "submenu": [
            {
              "label": "Toggle &Webapp DevTools",
              "command": "window:toggle-dev-tools",
              "accelerator": "Control+Alt+I",
              "id": "menuitem-toggle-webapp-devtools"
