        {
          "label": "&Paste as Plain Text",
          "command": "core:paste-and-match-style",
          "accelerator": "Control+Shift+V",
          "id": "menuitem-paste-as"
        },
        {
          "label": "Select &All",
          "command": "core:select-all",
          "accelerator": "Control+A",
          "id": "menuitem-select-all"
        },
        {
          "label": "Delete",
          "command": "core:delete",
          "accelerator": "Delete",
          "id": "menuitem-delete"
        },
        {
          "type": "separator"
        },
        {
          "label": "Find…",
          "command": "core:find",
          "accelerator": "Control+F",
          "id": "menuitem-find"
        }
      ]
    },
    {
      "label": "&View",
      "id": "menuitem-view",
      "submenu": [
        {
          "label": "&Reload",
          "command": "window:reload",
          "accelerator": "Control+R",
          "id": "menuitem-reload-current"
        },
        {
          "label": "Toggle &Full Screen",
          "command": "window:toggle-full-screen",
          "accelerator": "Control+Shift+F",
          "id": "menuitem-toggle-fullscreen"
