{
  "menu": [
    {
      "label": "&File",
      "submenu": [
        {
          "label": "&Preferences…",
          "command": "application:show-settings",
          "accelerator": "Control+,",
          "id": "menuitem-preferences"
        },
        {
          "type": "separator"
        },
        {
          "label": "&Close",
          "role": "close",
          "accelerator": "Control+W",
          "id": "menuitem-file-close"
        },
        {
          "label": "&Quit",
          "command": "application:quit",
          "accelerator": "Control+Q",
          "id": "menuitem-quit"
        }
      ]
    },
    {
      "label": "&Edit",
      "submenu": [
        {
          "label": "&Undo",
          "command": "core:undo",
          "accelerator": "Control+Z",
          "id": "menuitem-undo"
        },
        {
          "label": "&Redo",
          "command": "core:redo",
          "accelerator": "Control+Y",
          "id": "menuitem-redo"
        },
        {
          "type": "separator"
        },
        {
          "label": "&Cut",
          "command": "core:cut",
          "accelerator": "Control+X",
          "id": "menuitem-cut"
        },
        {
          "label": "C&opy",
          "command": "core:copy",
          "accelerator": "Control+C",
          "id": "menuitem-copy"
        },
        {
          "label": "&Paste",
          "command": "core:paste",
          "accelerator": "Control+V",
          "id": "menuitem-paste"
