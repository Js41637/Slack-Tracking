{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["es2015", "stage-2", "react"],
        "plugins": ["transform-runtime", "rewire"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["es2015", "stage-2", "react"],
        "plugins": ["transform-runtime"],
        "sourceMaps": false
      }
    }
  }
}
