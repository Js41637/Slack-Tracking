  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run build",
    "postversion": "./script/postversion.sh",
    "prepare-release": "babel-node ./script/prepare-release.js",
    "unversion": "./script/unversion.sh",
    "install": "babel-node ./build/bootstrap-cli.js",
    "package": "babel-node ./build/package-cli.js",
    "shame": "eslint ./src && eslint ./spec && eslint ./build && tslint -c ./tslint.json -p ./tsconfig.json 'src/**/*.ts' 'src/**/*.tsx' 'spec/**/*.ts'",
    "start": "cross-env DEBUG='logger:*,electron-spellchecker:*' electron . -r . --enable-logging",
    "mocha:electron": "electron-mocha --recursive --require=./spec/mocha/support.js --reporter dot --bail",
    "test:mock": "shx rm -rf ./.tmp/build/spec/jest/__mocks__ && jest --config jest.json --no-cache",
    "test:electron:browser": "npm-run-all \"mocha:electron -- ./spec/mocha/browser\"",
    "test:electron:renderer": "npm-run-all \"mocha:electron -- --renderer ./spec/mocha/renderer\"",
    "test": "npm-run-all test:**",
    "coverage": "npm-run-all \"test:mock -- --coverage --no-cache\" && shx cat ./coverage/remapped/coverage.txt",
    "build": "shx rm -rf ./.tmp && tsc && madge ./.tmp/build/src --circular",
    "docs": "babel-node ./build/generate-docs-cli.js",
    "licenses": "babel-node ./build/generate-licenses-cli.js",
    "copyright": "babel-node ./build/check-copyright-cli.js",
    "node": "cross-env ELECTRON_RUN_AS_NODE=1 ./node_modules/electron-prebuilt-compile/node_modules/.bin/electron"
  },
