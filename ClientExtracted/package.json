  "scripts": {
    "precommit": "lint-staged",
    "prepush": "npm run build",
    "danger": "danger run -c ./danger-build.js",
    "postversion": "./script/postversion.sh && npm run docs",
    "prepare-release": "ts-node ./script/prepare-release.js",
    "unversion": "./script/unversion.sh",
    "prepare": "ts-node ./build/bootstrap-cli.js",
    "package": "ts-node ./build/package-cli.js",
    "shame": "eslint ./src && eslint ./spec && eslint ./build && tslint -c ./tslint.json -p ./tsconfig.json \"src/**/*.ts\" \"src/**/*.tsx\" \"spec/**/*.ts\"",
    "start": "cross-env DEBUG='logger:*,electron-spellchecker:*' electron . -r . --enable-logging",
    "start:react-dev": "react-devtools",
    "start:all": "npm-run-all --parallel start:react-dev start",
    "mocha:electron": "electron-mocha --recursive --require=./spec/mocha/support.js --reporter dot --bail",
    "test:mock": "shx rm -rf ./.tmp/build/spec/jest/__mocks__ && jest --config jest.json --no-cache",
    "test:electron:browser": "npm run mocha:electron -- \"./spec/mocha/browser/**/*.ts\"",
    "test:electron:renderer": "npm-run-all \"mocha:electron -- --renderer ./spec/mocha/renderer\"",
    "test": "npm-run-all test:**",
    "coverage": "npm-run-all \"test:mock -- --coverage\"",
    "build": "shx rm -rf ./.tmp && tsc && madge ./.tmp/build/src --circular --break-on-error",
    "licenses": "ts-node ./build/generate-licenses-cli.js",
    "copyright": "ts-node ./build/check-copyright-cli.js",
    "node": "cross-env ELECTRON_RUN_AS_NODE=1 ./node_modules/electron-prebuilt-compile/node_modules/.bin/electron",
    "clone-docs": "shx rm -rf desktop-docs && git clone https://slack-github.com/slack/desktop-docs",
    "update-docs": "ts-node ./script/generate-docs.ts",
    "docs": "npm run clone-docs && npm run update-docs"
  },
